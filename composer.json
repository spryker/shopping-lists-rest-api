{
  "name": "spryker/shopping-lists-rest-api",
  "type": "library",
  "description": "ShoppingListsRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/company-user": "^2.0.0",
    "spryker/glue-application": "^1.9.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/shopping-list": "^2.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/rest-request-validator": "If you want to use rest request validation.",
    "spryker/company-users-rest-api": "If you want to use company user header validation.",
    "spryker/products-rest-api": "If you want to include concrete products in response."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
